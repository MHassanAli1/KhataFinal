{
  "name": "khata",
  "version": "1.0.0",
  "description": "An Electron application with React",
  "main": "out/main/index.js",
  "author": "Caches",
  "homepage": "https://electron-vite.org",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build && npm run copy:package && npm run copy:prisma",
    "postinstall": "",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true }); require('fs').rmSync('out', { recursive: true, force: true }); console.log('Cleaned dist and out directories');\"",
    "copy:package": "node scripts/copy-package-json.js",
    "copy:prisma": "node scripts/copy-prisma.js",
    "build:unpack": "npm run clean && npm run build && electron-builder --dir",
    "build:win": "npm run clean && npm run build && electron-builder --win",
    "build:mac": "npm run clean && npm run build && electron-builder --mac",
    "build:linux": "npm run clean && npm run build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@prisma/client": "^6.9.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.4.9",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "khata": "file:",
    "lucide-react": "^0.511.0",
    "node-fetch": "^3.3.2",
    "react-chartjs-2": "^5.3.0",
    "react-router-dom": "^7.6.3",
    "react-simple-keyboard": "^3.8.80",
    "rechart": "^0.0.1",
    "recharts": "^2.15.3",
    "simple-keyboard": "^3.8.59"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fs-extra": "^11.3.0",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vite": "^6.2.6"
  }
}
